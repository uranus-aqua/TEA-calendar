{
  "name": "tea-calendar",
  "private": false,
  "version": "1.1.3",
  "description": "Pragmatically convert Gregorian calendar date (1583-2100) to Chinese calendar date",
  "exports": {
    "import": "./dist/teac.js",
    "require": "./dist/teac.umd.cjs",
    "default": "./src/index.js"
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "jsdelivr": "./dist/teac.js",
  "unpkg": "./dist/teac.js",
  "main": "./dist/teac.umd.cjs",
  "module": "./dist/teac.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build --force",
    "preview": "vite preview",
    "test": "nyc --reporter=html --reporter=text mocha './test/*.test.*js' --reporter doc --timeout=12000"
  },
  " ": {
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "vite": "^4.4.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uranus-aqua/tea-calendar.git"
  },
  "keywords": [
    "calendar",
    "Chinese",
    "conversion",
    "Gregorian"
  ],
  "author": {
    "name" : "KUO Wei-Shiung",
    "url" : "https://kws.baseed.net/"
  },
  "bugs": {
    "url": "https://github.com/uranus-aqua/tea-calendar/issues"
  },
  "homepage": "https://github.com/uranus-aqua/tea-calendar#readme",
  "devDependencies": {
    "debug": "^4.3.4",
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "rollup-plugin-terser": "^7.0.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "vite": "^4.4.9",
    "rollup": "^9.8.1"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
